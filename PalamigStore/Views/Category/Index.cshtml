@model IEnumerable<Category>

<h1>Category List</h1>
<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model) //Accessing the list
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Quantity</td>
            </tr>
        }
    </tbody>
</table>
